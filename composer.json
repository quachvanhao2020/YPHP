{
    "name": "hao/yphp",
    "type": "library",
    "description": "YPHP",
    "keywords": ["php", "base", "core", "model"],
    "authors": [
        {
            "name": "vanhaohero1998",
            "email": "quachvanhao2020@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "YPHP\\": "src/",
            "Doctrine\\": "Doctrine/",
            "Laminas\\": "Laminas/",
            "Symfony\\": "Symfony/"
        },
        "files": [
            "src/define.php",
            "src/functions.php",
            "src/awake.php"
        ]
    },
    "require": {
        "php": ">=5.3",
        "psr/http-message": "^1.0",
        "psr/log": "^1.1",
        "psr/container": "^1.0",
        "psr/simple-cache": "^1.0",
        "netresearch/jsonmapper": "^3.1",
        "laminas/laminas-http": "^2.14",
        "laminas-api-tools/api-tools-provider": "^1.2",
        "laminas/laminas-paginator": "^2.9",
        "laminas/laminas-hydrator": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "doctrine/orm": "^2.8"
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
